{
    "name": "Levels Checker",
    "short_name": "Levels Checker",
    "description": "Checks if the current site has their salary information on levels.fyi",
    "homepage_url": "https://github.com/jameszhang-a/levels-checker",
    "version": "1.0.0",
    "version_name": "1,0,0",
    "manifest_version": 3,
    "default_locale": "en",
    "minimum_chrome_version": "88",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "icons": {
        "128": "assets/img/128x128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Levels",
        "default_popup": "index.html",
        "default_icon": {
            "16": "assets/img/16x16.png",
            "24": "assets/img/24x24.png",
            "32": "assets/img/32x32.png"
        }
    }
}