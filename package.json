{
    "name": "levels-check",
    "description": "stuff",
    "version": "0.0.1",
    "homepage": "http://chrome.google.com/webstore",
    "author": "James Zhang",
    "repository": {
        "type": "git",
        "url": "ENTER GIT REPO URL"
    },
    "scripts": {
        "start": "xt-build -e dev -p chrome -w",
        "build": "xt-build -e prod -p chrome",
        "clean": "xt-clean",
        "docs": "xt-docs",
        "test": "xt-test",
        "coverage": "nyc --reporter=lcov npm run test",
        "sync": "xt-sync"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "eslintIgnore": [
        "test/**/*"
    ],
    "devDependencies": {
        "extension-cli": "latest"
    },
    "xtdocs": {
        "source": {
            "include": [
                "README.md",
                "src"
            ]
        }
    },
    "xtbuild": {
        "js_bundles": [
            {
                "name": "background",
                "src": "./src/background.js"
            },
            {
                "name": "app",
                "src": "./src/app/**/*.js"
            }
        ],
        "manifest": "./src/manifest.json"
    }
}